	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{jwt}}",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const loginUrl = pm.environment.get(\"baseUrl\") + \"/authenticate\";",
					"const username = pm.environment.get(\"username\");",
					"const password = pm.environment.get(\"password\");",
					"const credentials = {\"username\": String(username), \"password\": String(password)};",
					"pm.sendRequest({",
					"    url: loginUrl,",
					"    method: 'POST',",
					"    header: {",
					"        'content-type': 'application/json'",
					"    },",
					"    body: {",
					"        mode: 'raw',",
					"        raw: JSON.stringify(credentials)",
					"        ",
					"    }",
					"}, function (err, res) {",
					"    pm.environment.set(\"jwt\", res.text());",
					"\t});"
				]
			}
		},
